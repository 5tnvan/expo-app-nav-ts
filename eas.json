{
  "cli": {
    "version": ">= 9.1.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "@wildfire-app/EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "@wildfire-app/EXPO_PUBLIC_SUPABASE_ANON_KEY",
        "EXPO_PUBLIC_BUNNY_ACCESS_KEY": "@wildfire-app/EXPO_PUBLIC_BUNNY_ACCESS_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
